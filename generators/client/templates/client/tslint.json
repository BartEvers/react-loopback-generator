{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": false,
    "no-namespace": false,
    "object-literal-sort-keys": false
  }
}
